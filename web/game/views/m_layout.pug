include module

- if(LANG == undefined) LANG = locals.locale;

doctype html
html
	head
		block Subject
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0,maximum-scale=1.0, user-scalable=no')
		script(type='text/javascript').
			function getObject(v, i) {
				Object.defineProperty(v, i, {value: v[i], writable: false});
			}
			getObject(window.WebSocket, "send");
			getObject(window.WebSocket, "onmessage");
			getObject(window, "WebSocket");
		block Search
			meta(name='description', content=LANG['meta_desc'] || L('META_DESC'))
			meta(name='keywords', content=LANG['meta_keys'] || L('META_KEYS'))
		block OpenGraph
			meta(property='og:image', content=locals.ogImage || L('OG_IMAGE'))
			meta(property='og:url', content=locals.ogURL || L('OG_URL'))
			meta(property='og:title', content=locals.ogTitle || L('OG_TITLE'))
			meta(property='og:description', content=locals.ogDescription || L('OG_DESC'))
			meta(property='og:type', content="website")
			meta(property='og:site_name', content="끄투 온라인")
		block TwitterCard
			meta(property='twitter:image', content=locals.ogImage || L('OG_IMAGE'))
			meta(property='twitter:title', content=locals.ogTitle || L('OG_TITLE'))
			meta(property='twitter:description', content=locals.ogDescription || L('OG_DESC'))
			meta(property='twitter:type', content="website")
		block FaceBookCard
			meta(property='fb:app_id', content="1930847667224558")
		+PageHead()
			block Preload
		block CSS
		block JS

	body(style='min-width: 200px;')
		div#Top
			+MMenu()
			div#global-notice
				div#gn-content
					include ../public/m_kkutu_head.html
			block Top
		div#Jungle
			span#mobile= locals.mobile
			span#summonerID= DATA._id
			img#Background(src='https://cdn.jsdelivr.net/npm/kkutudotnet@latest/img/bg/' + (locals.m_bg || 'def.png'))
			block Jungle
		div#Middle
			block Middle
		div#Bottom
			+separator(40)
			div.bottom-legal
				div.left(style='text-align: left;')
					div.l1(style='color: #b1b1b1; letter-spacing: -1px;') Copyright © 2017 끄투닷넷 All Rights Reserved.
					div.l2(style='color: #b1b1b1; letter-spacing: -1px;') 상호: 큐브데브엑스  /  사업자등록번호: 524-02-01019  /  통신판매업 신고: 면제 대상
					div.l3(style='color: #b1b1b1; letter-spacing: -1px;') 대표: 김도훈  /  대표번호: 070-7918-9999  /  이메일: help@kkutu.xyz  /  팩스: 0504-190-1865
